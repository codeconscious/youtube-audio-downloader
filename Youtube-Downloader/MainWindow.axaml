<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="250"
        x:Class="Youtube_Downloader.MainWindow"
        Title="YouTube Downloader">
    <StackPanel Orientation="Horizontal" Spacing="5" Height="20">
        <TextBlock Margin="4" Text="https://www.youtube.com/"></TextBlock>
        <TextBox Watermark="Enter URL part" Width="200" x:Name="Url"></TextBox>
    </StackPanel>
</Window>
